<van-cell-group title="仪器运行情况设置">
  <van-cell>
    <view slot="title" style="line-height:60rpx">
      电仪器的预约位置数
    </view>
    <van-stepper slot="right-icon" value="{{ roomSeats[0] }}" min="{{minSeat}}" max="{{maxSeat}}" bind:change="onSeatsChange" data-roomid="a" disable-input async-change />
  </van-cell>
  <van-cell>
    <view slot="title" style="line-height:60rpx">
      磁仪器的预约位置数
    </view>
    <van-stepper slot="right-icon" value="{{ roomSeats[1] }}" min="{{minSeat}}" max="{{maxSeat}}" bind:change="onSeatsChange" data-roomid="b" disable-input async-change />
  </van-cell>
</van-cell-group>
<van-dialog id="van-dialog" />
<van-toast id="van-toast" />

